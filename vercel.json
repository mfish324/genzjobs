{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "prisma generate && next build",
  "crons": [
    {
      "path": "/api/cron/scrape-ats",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/geocode",
      "schedule": "0 */6 * * *"
    }
  ]
}
